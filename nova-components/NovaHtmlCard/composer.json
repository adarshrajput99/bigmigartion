{
    "name": "interaction-design-foundation/nova-html-card",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "InteractionDesignFoundation\\NovaHtmlCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InteractionDesignFoundation\\NovaHtmlCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
